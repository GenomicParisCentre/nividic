<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://www.example.org/lims/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="lims"
	targetNamespace="http://www.example.org/lims/">
	<wsdl:types>
		<xsd:schema targetNamespace="http://www.genomic.ens.fr/lims/"
			xmlns:tns="http://www.genomic.ens.fr/lims/">
			<xsd:element name="getHybridatzation" type="tns:null">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SlideSerialNumber"
							type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="getHybridatzationResponse"
				type="tns:null">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="out" type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:complexType name="Hydridization">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:int" maxOccurs="1"
						minOccurs="1" />

					<xsd:element name="date" type="xsd:date"
						maxOccurs="1" minOccurs="1" />

					<xsd:element name="owner" type="xsd:string"
						maxOccurs="1" minOccurs="0" />

					<xsd:element name="slideSerialNumber"
						type="xsd:string" maxOccurs="1" minOccurs="0" />

					<xsd:element name="name" type="xsd:string"
						maxOccurs="1" minOccurs="1" />

					<xsd:element name="comment" type="xsd:string"
						maxOccurs="1" minOccurs="0" />

					<xsd:element name="organism" type="xsd:int"
						maxOccurs="1" minOccurs="0" />

					<xsd:element name="slideId" type="xsd:string"
						maxOccurs="1" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="Target">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:int" maxOccurs="1"
						minOccurs="1" />
					<xsd:element name="comment" type="xsd:string"
						maxOccurs="1" minOccurs="0" />
					<xsd:element name="dye" type="xsd:string"
						maxOccurs="1" minOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>



			<xsd:complexType name="Targets">
				<xsd:sequence>
					<xsd:element name="targets" type="tns:Target"
						maxOccurs="unbounded" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="Scan">
				<xsd:sequence>
					<xsd:element name="hybridizationId" type="xsd:int"
						maxOccurs="1" minOccurs="1" />

					<xsd:element name="creationDate" type="xsd:string"
						maxOccurs="1" minOccurs="0" />

					<xsd:element name="modificationDate"
						type="xsd:string" maxOccurs="1" minOccurs="0" />

					<xsd:element name="id" type="xsd:int" maxOccurs="1"
						minOccurs="1" />

					<xsd:element name="comment" type="xsd:string"
						maxOccurs="1" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="Scans">
				<xsd:sequence>
					<xsd:element name="scans" type="tns:Scan"
						maxOccurs="unbounded" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="getHybridizationRequest">
		<wsdl:part name="slideSerialNumber" type="xsd:string" />
	</wsdl:message>
	<wsdl:message name="getHybridizationResponse">
		<wsdl:part name="parameters" type="tns:Hydridization" />
	</wsdl:message>
	<wsdl:message name="NewOperationRequest">
		<wsdl:part name="parameters" element="tns:NewOperation" />
	</wsdl:message>
	<wsdl:message name="NewOperationResponse">
		<wsdl:part name="parameters" element="tns:NewOperationResponse" />
	</wsdl:message>
	<wsdl:message name="getTargetsRequest1">
		<wsdl:part name="hybridizationId" type="xsd:int" />
	</wsdl:message>
	<wsdl:message name="getTargetsResponse1">
		<wsdl:part name="getTargetsResponse1" type="tns:Targets" />
	</wsdl:message>
	<wsdl:message name="getScansRequest1">
		<wsdl:part name="hybridizationId" type="xsd:int" />
	</wsdl:message>
	<wsdl:message name="getScansResponse1">
		<wsdl:part name="getScansResponse1" type="tns:Scans" />
	</wsdl:message>
	<wsdl:portType name="lims">
		<wsdl:operation name="getHybridization">
			<wsdl:input message="tns:getHybridizationRequest" />
			<wsdl:output message="tns:getHybridizationResponse" />
		</wsdl:operation>
		<wsdl:operation name="getTargets">
			<wsdl:input message="tns:getTargetsRequest1" />
			<wsdl:output message="tns:getTargetsResponse1" />
		</wsdl:operation>
		<wsdl:operation name="getScans">
			<wsdl:input message="tns:getScansRequest1" />
			<wsdl:output message="tns:getScansResponse1" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="limsSOAP" type="tns:lims">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="getHybridization">
			<soap:operation
				soapAction="http://www.example.org/lims/NewOperation" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="lims">
		<wsdl:port binding="tns:limsSOAP" name="limsSOAP">
			<soap:address location="http://www.genomic.ens.fr/lims" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
